{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/": {
      "get": {
        "summary": "Read Root",
        "operationId": "read_root__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          }
        }
      }
    },
    "/items/{item_id}": {
      "get": {
        "summary": "Read Item",
        "operationId": "read_item_items__item_id__get",
        "parameters": [
          {
            "name": "item_id",
            "in": "path",
            "required": true,
            "schema": { "type": "integer", "title": "Item Id" }
          },
          {
            "name": "q",
            "in": "query",
            "required": false,
            "schema": { "type": "string", "default": "", "title": "Q" }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/extract_keywords": {
      "post": {
        "summary": "Extract Keywords Api",
        "operationId": "extract_keywords_api_extract_keywords_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/TextRequest" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    { "$ref": "#/components/schemas/ExtractKeywordsSuccess" },
                    { "$ref": "#/components/schemas/ExtractKeywordsError" }
                  ],
                  "title": "Response Extract Keywords Api Extract Keywords Post"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ExtractKeywordsError": {
        "properties": {
          "status": { "type": "string", "title": "Status" },
          "message": { "type": "string", "title": "Message" }
        },
        "type": "object",
        "required": ["status", "message"],
        "title": "ExtractKeywordsError"
      },
      "ExtractKeywordsSuccess": {
        "properties": {
          "status": { "type": "string", "title": "Status" },
          "keywords": {
            "items": { "$ref": "#/components/schemas/Keyword" },
            "type": "array",
            "title": "Keywords"
          }
        },
        "type": "object",
        "required": ["status", "keywords"],
        "title": "ExtractKeywordsSuccess"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Keyword": {
        "properties": {
          "word": { "type": "string", "title": "Word" },
          "count": { "type": "integer", "title": "Count" }
        },
        "type": "object",
        "required": ["word", "count"],
        "title": "Keyword"
      },
      "TextRequest": {
        "properties": {
          "text": { "type": "string", "title": "Text" },
          "top_n": { "type": "integer", "title": "Top N", "default": 5 }
        },
        "type": "object",
        "required": ["text"],
        "title": "TextRequest"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
